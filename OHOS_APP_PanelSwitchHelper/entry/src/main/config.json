{
  "app": {
    "bundleName": "com.example.mydome",
    "vendor": "example",
    "version": {
      "code": 1,
      "name": "1.0"
    },
    "apiVersion": {
      "compatible": 4,
      "target": 4,
      "releaseType": "Beta2"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.example.mydome",
    "name": ".MyApplication",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "orientation": "unspecified",
        "name": "com.example.mydome.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "MyDome",
        "type": "page",
        "launchType": "standard"
      }, {
        "orientation": "unspecified",
        "name": "com.example.mydome.TransitAbility",
        "icon": "$media:icon",
        "description": "$string:transitability_description",
        "label": "entry",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.example.mydome.ChatAbility",
        "icon": "$media:icon",
        "description": "$string:chatability_description",
        "label": "entry",
        "type": "page",
        "launchType": "standard"
      },
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "name": "com.example.mydome.HomeAbility",
        "icon": "$media:icon",
        "description": "$string:homeability_description",
        "label": "entry",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "metaData": {
      "customizeData": [
        {
          "name": "hwc-theme",
          "value": "androidhwext:style/Theme.Emui.NoTitleBar"
        }
      ]
    }
  }
}